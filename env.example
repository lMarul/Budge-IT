# Environment Variables for Budge-IT Application
# Copy this file to .env and fill in your actual values

# Flask Secret Key (generate a secure random key)
SECRET_KEY=your-super-secure-secret-key-here

# Supabase Database Connection (from your Supabase project settings)
DATABASE_URL=postgresql://postgres:[YOUR_PASSWORD]@[YOUR_HOST]:5432/postgres

# Optional: Debug mode (set to True for development, False for production)
DEBUG=False

# Optional: Port for local development
PORT=5001 